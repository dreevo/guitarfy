<h2 class="page-header">Search Guitars</h2>
<form action="/guitars" method="GET">
  <div class="form-row">
    <div class="form-item">
      <label for="name">Name</label>
      <input type="text" name="name" value="<%= searchOptions.name %>" />
    </div>
  </div>
  <div class="form-row">
    <div class="form-item">
      <label for="manufacturedAfter">Manufactured after</label>
      <input
        type="date"
        name="manufacturedAfter"
        value="<%= searchOptions.manufacturedAfter %>"
      />
    </div>
    <div class="form-item">
      <label for="manufacturedBefore">Manufactured Before</label>
      <input
        type="date"
        name="manufacturedBefore"
        value="<%= searchOptions.manufacturedBefore %>"
      />
    </div>
  </div>
  <div class="form-row form-row-end">
    <button class="btn btn-primary" type="submit">Search</button>
  </div>
</form>
<br />
<%- include('../partials/guitarGrid', {guitars }) %>
